{
  "name": "nansen-cli",
  "version": "1.3.3",
  "description": "Command-line interface for Nansen API - designed for AI agents",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "nansen": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
